<view class='amountBg'>
  <view class='img'>
    <open-data type="userAvatarUrl"></open-data>
  </view>
  <view class='account'>
    <view class='nick-name'>
      <open-data type="userNickName" lang="zh_CN"></open-data>
    </view>
    <view class='address'>
      <open-data type="userCountry" lang="zh_CN"></open-data>·
      <open-data type="userProvince" lang="zh_CN"></open-data>
    </view>
  </view>
</view>
<van-dialog id="van-dialog" show="{{ show }}" show-cancel-button message="您尚未注册，是否前去注册" bind:close="dialogClose_reg" bind:confirm="dialogConfirm_reg"/>
<view class="me">
  <van-grid> 
  <van-grid-item class="my_info" icon="photo-o" bind:click="goInfo" text="我的信息" />
  <van-grid-item class="my_pub" icon="photo-o" link-type="navigateTo" url="/pages/me/released" text="我的发布" />
</van-grid>
</view>


